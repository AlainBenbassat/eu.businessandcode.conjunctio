<?xml version="1.0"?>
<extension key="eu.businessandcode.conjunction" type="module">
  <file>conjunction</file>
  <name>Conjunction</name>
  <description>Find contacts by tag using AND (not OR)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alain Benbassat</author>
    <email>alain@businessandcode.eu</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/AlainBenbassat/eu.businessandcode.conjunction.git</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2018-10-27</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>4.7 5.x</ver>
  </compatibility>
  <comments>Default search combines tags with OR, this extension uses AND.</comments>
  <civix>
    <namespace>CRM/Conjunction</namespace>
  </civix>
</extension>
